{
    "name": "mk-vue",
    "private": true,
    "version": "1.0.1",
    "description": "vite + vue + ts项目基础配置",
    "warName": "vite",
    "homepage": "https://mankeung.github.io/docs/mk-vue",
    "repository": {
        "type": "git",
        "url": "https://github.com/mankeung/mk-vue"
    },
    "author": "mankeung",
    "keywords": [
        "vue",
        "vite",
        "typescript",
        "ts",
        "pinia",
        "axios"
    ],
    "scripts": {
        "docs:dev": "vuepress dev docs",
        "docs:build": "vuepress build docs",
        "dev": "cross-env NODE_ENV=dev vite --mode dev",
        "build:test": "vue-tsc --noEmit && cross-env NODE_ENV=test vite build --mode test",
        "build": "vue-tsc --noEmit && cross-env NODE_ENV=prod vite build --mode prod",
        "preview": "vite preview",
        "lint:eslint": "eslint --fix \"src/**/*.{vue,ts,js}\"",
        "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
        "lint:style": "stylelint --fix \"src/**/*.{scss,less,vue,css}\" --cache --cache-location node_modules/.cache/stylelint/",
        "lint": "pnpm lint:prettier && pnpm lint:style && pnpm lint:eslint",
        "prepare": "node ./scripts/prepare.js",
        "precommit": "node ./scripts/precommit.js",
        "war": "node ./scripts/war.js",
        "new": "plop"
    },
    "dependencies": {
        "amfe-flexible": "^2.2.1",
        "axios": "^0.27.2",
        "crypto-js": "^4.1.1",
        "hotkeys-js": "^3.10.0",
        "jsencrypt": "^3.2.1",
        "mitt": "^3.0.0",
        "mk-vue": "link:",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "pinia": "^2.0.22",
        "pinia-plugin-persist": "^1.0.0",
        "vue": "^3.2.37",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.5",
        "xss": "^1.0.14"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@types/crypto-js": "^4.1.1",
        "@types/node": "^18.7.21",
        "@types/nprogress": "^0.2.0",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "@vitejs/plugin-legacy": "^2.2.0",
        "@vitejs/plugin-vue": "^3.1.0",
        "@vitejs/plugin-vue-jsx": "^2.0.1",
        "@vuepress/plugin-register-components": "2.0.0-beta.51",
        "@vuepress/plugin-search": "2.0.0-beta.51",
        "add": "^2.0.6",
        "chalk": "4",
        "cross-env": "^7.0.3",
        "enquirer": "^2.3.6",
        "eslint": "^8.24.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-vue": "^9.5.1",
        "folder-zip-sync": "^1.0.5",
        "husky": "^8.0.1",
        "mockjs": "^1.1.0",
        "plop": "^3.1.1",
        "postcss-html": "^1.5.0",
        "postcss-pxtorem": "^6.0.0",
        "postcss-scss": "^4.0.5",
        "prettier": "^2.7.1",
        "rollup-plugin-visualizer": "^5.8.2",
        "sass": "^1.55.0",
        "stylelint": "^14.12.1",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^28.0.0",
        "stylelint-order": "^5.0.0",
        "stylelint-scss": "^4.3.0",
        "terser": "^5.15.0",
        "typescript": "^4.6.4",
        "unplugin-auto-import": "^0.11.2",
        "unplugin-vue-components": "^0.22.7",
        "vite": "^3.1.0",
        "vite-plugin-banner": "^0.5.0",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-mock": "^2.9.6",
        "vite-plugin-progress": "^0.0.5",
        "vite-plugin-resolve-externals": "^0.2.2",
        "vite-plugin-restart": "^0.2.0",
        "vite-plugin-svg-icons": "^2.0.1",
        "vue-tsc": "^0.40.4",
        "vuepress": "2.0.0-beta.51",
        "vuepress-plugin-copy-code2": "2.0.0-beta.104",
        "web-vitals": "^3.0.3"
    }
}
